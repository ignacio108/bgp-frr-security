frr version 10.2.1
frr defaults traditional
hostname rI
log syslog informational
ip forwarding
no ipv6 forwarding
service integrated-vtysh-config
!
key chain 1
 key 1
  cryptographic-algorithm hmac-sha-256
  key-string clave
 exit
exit
!
interface eth1
 description NetI
 ip ospf area 1
exit
!
interface eth2
 description NetG-I-H
 ip ospf area 1
 ip ospf authentication key-chain 1
 ip ospf network point-to-multipoint
 no ip ospf passive
exit
!
interface lo
 ip ospf area 1
exit
!
router bgp 64513
 bgp router-id 4.4.4.6
 bgp confederation identifier 100
 bgp confederation peers 64512 64513 64514 64515 64516
 neighbor 4.4.4.4 remote-as 64513
 neighbor 4.4.4.4 update-source lo
 !
 address-family ipv4 unicast
  redistribute connected
 exit-address-family
exit
!
router ospf
 ospf router-id 12.12.12.12
 passive-interface default
 area 1 range 10.1.3.0/24
exit
!
