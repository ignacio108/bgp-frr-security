frr version 10.2.1
frr defaults traditional
hostname rB
log syslog informational
ip forwarding
no ipv6 forwarding
service integrated-vtysh-config
!
key chain 1
 key 1
  cryptographic-algorithm hmac-sha-256
  key-string clave
 exit
exit
!
interface eth3
 description NetB-E
 ip ospf area 0
 ip ospf authentication key-chain 1
 ip ospf network point-to-point
 no ip ospf passive
exit
!
interface eth4
 description NetB-G
 ip ospf area 0
 ip ospf authentication key-chain 1
 ip ospf network point-to-point
 no ip ospf passive
exit
!
interface lo
 ip ospf area 0
exit
!
router eigrp 100
 network 10.1.1.0/28
exit
!
router bgp 64515
 bgp router-id 5.5.5.5
 bgp confederation identifier 100
 bgp confederation peers 64512 64513 64514 64515 64516
 neighbor 3.3.3.3 remote-as 64514
 neighbor 3.3.3.3 update-source lo
 neighbor 4.4.4.4 remote-as 64513
 neighbor 4.4.4.4 update-source lo
exit
!
router ospf
 ospf router-id 5.5.5.5
 passive-interface default
 area 0 range 192.168.1.0/24
exit
!