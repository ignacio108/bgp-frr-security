frr version 10.2.1
frr defaults traditional
hostname rB
log syslog informational
ip forwarding
ipv6 forwarding
service integrated-vtysh-config
!
key chain 1
 key 1
  cryptographic-algorithm hmac-sha-256
  key-string clave
 exit
exit
!
interface eth1
 ip ospf area 0
 ip ospf passive
 ipv6 ospf6 area 0
 ipv6 ospf6 passive
exit
!
interface eth2
 ip ospf area 0
 ip ospf authentication key-chain 1
 ip ospf network point-to-point
 ipv6 ospf6 area 0
 ipv6 ospf6 authentication keychain 1
 ipv6 ospf6 network point-to-point
exit
!
interface eth3
 ip ospf area 0
 ip ospf authentication key-chain 1
 ip ospf network point-to-point
 ipv6 ospf6 area 0
 ipv6 ospf6 authentication keychain 1
 ipv6 ospf6 network point-to-point
exit
!
interface lo
 ip address 10.7.0.2/24
 ip ospf area 0
 ip ospf passive
 ipv6 address fd80:e42c:3ce3::2/128
 ipv6 ospf6 area 0
exit
!
router ospf
 ospf router-id 10.7.0.2
 area 0 range 10.8.1.0/24
exit
!
router ospf6
 ospf6 router-id 10.7.0.2
 area 0 range fd80:e42c:3ce3:a770::/64
exit
!


