frr version 10.2.1
frr defaults traditional
hostname rD
log syslog informational
ip forwarding
ipv6 forwarding
service integrated-vtysh-config
!
key chain 1
 key 1
  cryptographic-algorithm hmac-sha-256
  key-string clave2
 exit
exit
!
interface eth1
 ip ospf area 1
 ip ospf authentication key-chain 1
 ip ospf network point-to-multipoint
 ipv6 ospf6 area 1
 ipv6 ospf6 authentication keychain 1
 ipv6 ospf6 network point-to-multipoint
exit
!
interface eth2
 ip ospf area 1
 ip ospf passive
 ipv6 ospf6 area 1
 ipv6 ospf6 passive
exit
!
interface lo
 ip address 10.7.0.4/24
 ip ospf area 1
 ip ospf passive
 ipv6 address fd80:e42c:3ce3::4/128
 ipv6 ospf6 area 1
exit
!
router ospf
 ospf router-id 10.7.0.4
 area 1 range 10.7.1.0/24
exit
!
router ospf6
 ospf6 router-id 10.7.0.4
 area 1 range fd80:e42c:3ce3:a770::/64
exit
!

