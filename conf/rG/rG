frr version 10.2.1
frr defaults traditional
hostname rG
log syslog informational
ip forwarding
no ipv6 forwarding
service integrated-vtysh-config
!
key chain 1
 key 1
  cryptographic-algorithm hmac-sha-256
  key-string clave
 exit
exit
!
interface eth1
 description NetB-G
 ip ospf area 0
 ip ospf authentication key-chain 1
 ip ospf network point-to-point
 no ip ospf passive
exit
!
interface eth2
 description NetG-I-H
 ip ospf area 1
 ip ospf authentication key-chain 1
 ip ospf network point-to-multipoint
 no ip ospf passive
exit
!
interface eth3
 description NetG-K
 ip ospf area 0
 ip ospf authentication key-chain 1
 ip ospf network point-to-point
 no ip ospf passive
exit
!
interface lo
 ip ospf area 0
exit
!
router bgp 64513
 bgp router-id 4.4.4.4
 bgp confederation identifier 100
 bgp confederation peers 64512 64513 64514 64515 64516
 neighbor 2.2.2.2 remote-as 64516
 neighbor 2.2.2.2 update-source lo
 neighbor 5.5.5.5 remote-as 64515
 neighbor 5.5.5.5 update-source lo
exit
!
router ospf
 ospf router-id 4.4.4.4
 passive-interface default
 area 0 range 192.168.1.0/24
 area 1 range 10.1.3.0/24
exit
!